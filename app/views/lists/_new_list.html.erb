<div>
  <%= form_for list,  url: {action: "create"} do |f| %>
    <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="field">
      <%= f.label :description%>
      <%= f.text_field :description %>
     </div>
        <%= f.submit "Create list", class: "btn btn-primary" %>
    <% end %>
</div>